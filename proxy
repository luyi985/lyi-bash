
function setProxy() {
	MY_PROXY="http://Lyi:zxcASDqwe123456@sydproxy.acp.net:8080"
	export http_proxy="${MY_PROXY}"
	export https_proxy="${MY_PROXY}"
	export HTTP_PROXY="${MY_PROXY}"
	export HTTPS_PROXY="${MY_PROXY}"
	export no_proxy="127.0.0.1, localhost"
	unset PROXY
	echo "Proxy on"
}

function unsetProxy() {
	MY_PROXY="http://Lyi:zxcASDqwe123456@sydproxy.acp.net:8080"
	unset http_proxy
	unset https_proxy
	unset HTTP_PROXY
	unset HTTPS_PROXY
	unset no_proxy
	unset PROXY
	echo "Proxy off"
}

alias proxyOn=setProxy
alias proxyOff=unsetProxy